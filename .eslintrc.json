{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "process": false,
        "describe": false,
        "it": false,
        "xit": false,
        "Buffer": false,
        "__dirname": false,
        "BUNDLE_CSS": false
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2019
    },
    "plugins": [
        "svelte3"
    ],
    "overrides": [
        {
            "files": ["*.svelte"],
            "processor": "svelte3/svelte3"
        }
    ],
    "rules": {
        "no-console":0,
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}