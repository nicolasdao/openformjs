{
  "name": "openform",
  "version": "0.0.6",
  "description": "Front-end JS package that helps building forms.",
  "main": "public/build/bundle.mjs",
  "jsdelivr": "public/build/bundle.js",
  "files": [
    "public/build"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src/ --fix",
    "push": "git push --follow-tags origin master && npm publish",
    "rls": "standard-version --release-as",
    "start": "sirv public",
    "v": "node -e \"console.log(require('./package.json').version)\""
  },
  "devDependencies": {
    "@emotion/css": "^11.0.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "bootstrap": "^4.5.3",
    "eslint": "^7.14.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "standard-version": "^9.0.0",
    "svelte": "^3.0.0",
    "svelte-material-ui": "^1.0.0",
    "svelte-preprocess": "^4.6.1"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
